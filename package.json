{
  "name": "hummingbird",
  "version": "4.2.0",
  "description": "Support the drag and drop folder compression.",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leibnizli/hummingbird.git"
  },
  "author": "leibnizli",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leibnizli/hummingbird/issues"
  },
  "homepage": "https://github.com/leibnizli/hummingbird#readme",
  "build": {
    "appId": "dev.arayofsunshine.hummingbird",
    "mac": {
      "category": "tool",
      "target": {
        "target": "default",
        "arch": [
          "x64",
          "arm64"
        ]
      }
    },
    "asar": false
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "pack": "rm -rf ./dist && electron-builder --dir",
    "dist": "rm -rf ./dist && electron-builder",
    "build-watch": "rm -rf assets && webpack --watch",
    "build": "rm -rf assets && webpack",
    "dev": "electron .",
    "clean": "rm -rf ./out",
    "clean-dist": "rm -rf ./dist",
    "start": "electron-forge start",
    "package": "rm -rf ./out && electron-forge package",
    "make": "rm -rf ./out && export $(cat .env | xargs) && electron-forge make",
    "publish": "electron-forge publish"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-dmg": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/publisher-github": "^7.2.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^6.9.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "17.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "mini-css-extract-plugin": "^2.7.7",
    "style-loader": "^3.3.4",
    "terser": "^5.26.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.4.1",
    "gulp-htmlmin": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "imagemin": "^5.3.1",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^5.2.1",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-svgo": "^7.0.0",
    "imagemin-webp": "^5.0.0",
    "mime": "^3.0.0",
    "nconf": "0.11.4",
    "update-electron-app": "^3.0.0"
  }
}
